}
#box-footer-right {
	height: 11px;
	width: 10px;
	float: right;
	background-image: url(/m/default/media/images/sprite_horizontal.png);
	background-position: 190px -130px;
}
#box-footer-center {
	height: 11px;
	background-image: url(/m/default/media/images/sprite_horizontal.png);
	background-position: 0px -206px;
	background-repeat: repeat-x;
	overflow: hidden;
}
.button {
	height: 40px;
	background-image: url(/m/default/media/images/sprite_horizontal.png);
	background-position: right top;
	float: left;
	margin: 0 0 0 4px;
	cursor: pointer;
}
.button div {
	height: 40px;
	background-image: url(/m/default/media/images/sprite_horizontal.png);
	background-position: left top;
	float: left;
	margin: 0 4px 0 -4px;
	padding: 0 20px 0 24px;
	font-family: "HelveticaNeue-Medium", Helvetica, sans-serif;
	font-size: 14px;
	line-height: 38px;
	color: #6b6b6b;
	text-shadow: 0px 1px 0px #fff;
}
.button:hover {
	background-position: right -40px;
}
.button:hover div {
	background-position: left -40px;
}
.button:active {
	background-position: right -80px;
}
.button:active div {
	background-position: left -80px;
}
.input {
	*width: 273px;
	height: 38px;
	overflow: hidden;
	float: left;
	padding-right: 4px;
}
.input-left {
	width: 3px;
	height: 38px;
	background-image: url(/m/default/media/images/sprite_horizontal.png);
	background-position: -20px -120px;
	float: left;
}
.input-right {
	width: 3px;
	height: 38px;
	background-image: url(/m/default/media/images/sprite_horizontal.png);
	background-position: -24px -120px;
	float: right;
}
.input-center {
	height: 22px;
	background-image: url(/m/default/media/images/sprite_horizontal.png);
	background-position: 0px -158px;
	background-repeat: repeat-x;
	overflow: hidden;
	padding: 8px 7px;
}
.input input {
	border: none;
	-webkit-border-radius: none;
	border-radius: none;
	background: transparent;
	-webkit-box-shadow: none;
	box-shadow: none;
	padding: 0px;
	margin: 0px;
	height: 22px;
	line-height: 22px;
}