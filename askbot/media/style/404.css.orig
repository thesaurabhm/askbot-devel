}
#box-header-center {
	height: 10px;
	background-image: url(/m/default/media/images/sprite_horizontal.png);
	background-position: 0px -196px;
	background-repeat: repeat-x;
	overflow: hidden;
}
#box-content {
	
}
#box-content-left {
	background-image: url(/m/default/media/images/sprite_vertical.png);
	background-position: left top;
	background-repeat: repeat-y;
}
#box-content-right {
	background-image: url(/m/default/media/images/sprite_vertical.png);
	background-position: right top;
	background-repeat: repeat-y;
}
#box-content-center {
	margin: 0 10px;
