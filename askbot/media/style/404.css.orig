h1 {
	font-size: 55px;
	line-height: 55px;
	color: #444;
	font-family: "Ubuntu", sans-serif;
	font-weight: 700;
	margin: 0;
}
h3 {
	font-size: 16px;
	line-height: 16px;
	color: #888;
	font-family: "Ubuntu", sans-serif;
	font-weight: 500;
	margin: 0;
	padding-left: 3px;
}
h1.title {
	position: absolute;
	left: 353px;
	top: 170px;
}
h3.description {
	position: absolute;
	left: 353px;
	top: 222px;
}
#box {
	border: 1px solid #d0d0d0;
	background-color: #e5e5e5;
	-webkit-border-radius: 8px;
	border-radius: 8px;
	-webkit-box-shadow: inset 0px 1px 1px 0px #dcdcdc, 0px 1px 0px #fff;
	box-shadow: inset 0px 1px 1px 0px #dcdcdc, 0px 1px 0px #fff;
	padding: 3px;
	margin-bottom: 1px;
}
#inner-box {
	border: 1px solid #cecece;
	background-color: #fff;
	-webkit-border-radius: 5px;
	border-radius: 5px;
	padding: 20px;
	-webkit-box-shadow: 0px 1px 1px 0px #dcdcdc;
	box-shadow: 0px 1px 1px 0px #dcdcdc;
}
#question-mark {
	width: 77px;
	height: 150px;
	margin: 0 70px 0 50px;
	background-image: url(/m/default/media/images/question_mark.png);
	background-position: left center;
	background-repeat: no-repeat;
	float: left;
}
#divider {
	width: 1px;
	height: 150px;
	background: #e5e5e5;
	float: left;
	margin-right: 20px;
}
h2 {
	font-size: 17px;
	line-height: 17px;
	color: #616161;
	font-family: "Ubuntu", sans-serif;
	font-weight: 500;
	margin: 0;
}
#box ul {
	list-style: disc;
	list-style-position: inside;
	margin: 10px 0;
	padding: 0;
}
#box ul li {
	font-size: 14px;
	line-height: 24px;
	font-family: "HelveticaNeue", Helvetica, sans-serif;
	color: #636363;
}
input[type=text] {
	border: 1px solid #cbcbcb;
	-webkit-border-radius: 3px;
	border-radius: 3px;
	-webkit-box-shadow: inset 0px 1px 1px #ededed;
	box-shadow: inset 0px 1px 1px #ededed;
	padding: 9px;
	outline: none;
	font-family: "HelveticaNeue", Helvetica, sans-serif;
	font-size: 14px;
	line-height: 18px;
	color: #636363;
	width: 253px;
	margin: 0 2px 0 0;
}
#goog-wm-sb {
	height:40px;
}
::-webkit-input-placeholder {
	color: #b7b7b7;
}
:-moz-placeholder {  
	color: #b7b7b7;
}

nav {
	padding-top: 11px;
}
nav ul {
	text-align: center;
}
nav ul li {
	display: inline;
	color: #969696;
	font-size: 15px;
	font-family: "HelveticaNeue", Helvetica, sans-serif;
	text-decoration: none;
}
nav ul li a:link, nav ul li a:hover, nav ul li a:visited, nav ul li a:active {
	color: #969696;
	font-size: 15px;
	font-family: "HelveticaNeue", Helvetica, sans-serif;
	text-decoration: none;
}
nav ul li a:hover {
	color: #828282;
}
nav ul li.divider {
	padding: 0 14px;
}

